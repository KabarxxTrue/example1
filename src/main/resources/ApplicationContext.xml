<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="classicalMusic" class="com.kabarxx.example.ClassicalMusic"/>
    <bean id="rapMusic" class="com.kabarxx.example.RapMusic"/>
    <bean id="rockMusic" class="com.kabarxx.example.RockMusic"/>

    <bean id="musicPlayer" class="com.kabarxx.example.MusicPlayer">
        <property name="musicList">
            <list>
                <ref bean="classicalMusic"/>
                <ref bean="rapMusic"/>
                <ref bean="rockMusic"/>
            </list>
        </property>
    </bean>
</beans>